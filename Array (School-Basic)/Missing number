
class Compute {
    
    public static int missingNumber(int a[], int N)
    {
         // Your code goes here
         
         int sum = 0; 
         for (int i = 0; i < N ; i++)
         {
             sum = sum + a[i];
             
         }
         int suma = ((N+1)*N)/2;
         
         return suma - sum ;
    }
}
