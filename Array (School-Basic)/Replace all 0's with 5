
class GfG {
    int convertfive(int num) {
        // Your code here
        
        int rev=0;
		    int temp =num;
		    int mul=1;
		
		    while(temp>0) {
			
			    int last=temp%10;
			
			    if(last==0) {
				    last = 5;
			  }
			
			  rev=(last*mul)+rev;
			
			  mul=mul*10;
			
			  temp=temp/10;
		  }
		  return rev;
    }
}
